  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.8",
    "babel-eslint": "^8.2.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
